<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record
            id="stage_applicant"
            model="edu.stage">
            <field name="description">Подано заявление на приём</field>
            <field name="name">Новый</field>
            <field name="sequence">1</field>
            <field
                name="case_default"
                eval="True"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">draft</field>
        </record>

        <record
            id="stage_entrance_test"
            model="edu.stage">
            <field name="description">Допущен к вступительным испытаниям</field>
            <field name="name">На испытаниях</field>
            <field name="sequence">2</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">entrance</field>
        </record>

        <record
            id="stage_competition"
            model="edu.stage">
            <field name="description">Допущен к участию в конкурсе</field>
            <field name="name">На конкурсе</field>
            <field name="sequence">4</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">entrance</field>
        </record>

        <record
            id="stage_recommended"
            model="edu.stage">
            <field name="description">Рекомендован к зачислению</field>
            <field name="name">Рекомендован</field>
            <field name="sequence">6</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">entrance</field>
        </record>

        <record
            id="stage_enlisted"
            model="edu.stage">
            <field name="description">Зачислен на обучение</field>
            <field name="name">Зачислен</field>
            <field name="sequence">7</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_1"
            model="edu.stage">
            <field name="description">Обучение на первом курсе</field>
            <field name="name">Курс 1</field>
            <field name="sequence">10</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_2"
            model="edu.stage">
            <field name="description">Обучение на втором курсе</field>
            <field name="name">Курс 2</field>
            <field name="sequence">20</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_3"
            model="edu.stage">
            <field name="description">Обучение на третьем курсе</field>
            <field name="name">Курс 3</field>
            <field name="sequence">30</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_4"
            model="edu.stage">
            <field name="description">Обучение на четвертом курсе</field>
            <field name="name">Курс 4</field>
            <field name="sequence">40</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_5"
            model="edu.stage">
            <field name="description">Обучение на пятом курсе</field>
            <field name="name">Курс 5</field>
            <field name="sequence">50</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_6"
            model="edu.stage">
            <field name="description">Обучение на шестом курсе</field>
            <field name="name">Курс 6</field>
            <field name="sequence">60</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_7"
            model="edu.stage">
            <field name="description">Обучение на седьмом курсе</field>
            <field name="name">Курс 7</field>
            <field name="sequence">70</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="False"/>
            <field name="state">open</field>
        </record>

        <record
            id="stage_graduated"
            model="edu.stage">
            <field name="description">Обучение завершено</field>
            <field name="name">Выпущен</field>
            <field name="sequence">80</field>
            <field
                name="case_default"
                eval="True"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">done</field>
        </record>

        <record
            id="stage_ao"
            model="edu.stage">
            <field name="description">Академический отпуск</field>
            <field name="name">В отпуске</field>
            <field name="sequence">90</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">pending</field>
        </record>

        <record
            id="stage_pr"
            model="edu.stage">
            <field name="description">Обучение приостановлено</field>
            <field name="name">Не обучается</field>
            <field name="sequence">95</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">pending</field>
        </record>

        <record
            id="stage_dismissed"
            model="edu.stage">
            <field name="description">Обучение прекращено</field>
            <field name="name">Отчислен</field>
            <field name="sequence">100</field>
            <field
                name="case_default"
                eval="True"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">canceled</field>
        </record>

        <record
            id="stage_test_failed"
            model="edu.stage">
            <field name="description">Не прошёл вступительные испытания</field>
            <field name="name">Не прошёл испытания</field>
            <field name="sequence">110</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">entrance</field>
        </record>

        <record
            id="stage_competition_failed"
            model="edu.stage">
            <field name="description">Не прошёл по конкурсу</field>
            <field name="name">Не прошёл конкурс</field>
            <field name="sequence">120</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">entrance</field>
        </record>

        <record
            id="stage_canceled"
            model="edu.stage">
            <field name="description">Отказ от поступления</field>
            <field name="name">Отказ</field>
            <field name="sequence">130</field>
            <field
                name="case_default"
                eval="False"/>
            <field
                name="fold"
                eval="True"/>
            <field name="state">entrance</field>
        </record>

    </data>
</openerp>
